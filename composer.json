{
	"name": "uestla/recaptcha-control",
	"type": "library",
	"description": "reCAPTCHA control for Nette Framework forms",
	"keywords": ["nette", "forms", "recaptcha"],
	"homepage": "https://github.com/uestla/ReCaptchaControl",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/uestla/ReCaptchaControl/issues"
	},
	"require": {
		"php": ">=5.6.0",
		"nette/di": "^2.4",
		"nette/http": "^2.4",
		"nette/forms": "^2.4",
		"nette/utils": "^2.4",
		"nette/php-generator": "^2.4 || ^3.0"
	},
	"require-dev": {
		"php": ">= 7.0",
		"tracy/tracy": "^2.4",
		"latte/latte": "^2.4",
		"nette/tester": "^2.0",
		"nette/bootstrap": "^2.4",
		"guzzlehttp/guzzle": "^3.8"
	},
	"suggest": {
		"ext-curl": "Needed by the default ReCaptchaControl\\Http\\Requester"
	},
	"autoload": {
		"psr-0": {
			"ReCaptchaControl": "src/"
		}
	},
	"scripts": {
		"test": "tester tests/ -C"
	}
}
